<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <CheckBoxPreference
        android:key="isOpenBlackNum"
        android:title="设置是否黑名单拦截"
        android:summaryOn="黑名单拦截开启"
        android:summaryOff="黑名单拦截关闭"

        />

    <CheckBoxPreference
        android:key="isUpdate"
        android:title="设置是否自动更新"
        android:summaryOn="自动更新"
        android:summaryOff="不自动更新"
        android:defaultValue="true"
        />

    <CheckBoxPreference
        android:key="showAddress"
        android:title="设置显示号码归属地"
        android:summaryOn="开启"
        android:summaryOff="关闭"
        />

    <CheckBoxPreference
        android:key="watchdog"
        android:title="是否开启密码锁"
        android:summaryOn="开启"
        android:summaryOff="关闭"
        />


</PreferenceScreen>